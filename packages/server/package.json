{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run migrate && node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "migrate": "sequelize db:migrate",
    "lint": "eslint . && npm run prettier:check",
    "prettier": "prettier --write \"src/**/*.js\" \"db/**/*.js\"",
    "prettier:check": "prettier -c \"src/**/*.js\" \"db/**/*.js\"",
    "test": "NODE_ENV=test LOG_LEVEL=warn mocha test/*.js test/**/*.js --timeout 30000 --exit"
  },
  "dependencies": {
    "common": "^0.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^9.0.1",
    "ethers": "^5.1.4",
    "express": "~4.16.1",
    "express-validator": "^6.11.1",
    "ioredis": "^4.27.6",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "randomstring": "^1.2.1",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "serve-static": "^1.14.1",
    "web3": "^1.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3"
  },
  "eslintIgnore": [
    "node_modules",
    "bin"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "proseWrap": "always",
    "trailingComma": "none"
  }
}
